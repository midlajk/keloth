    <!-- Page CSS -->
    <style>
        .dropzone {
            /* Add your dropzone styles here */
            border: 2px dashed #ccc;
            padding: 20px;
            text-align: center;
        }

        .preview-container {
            /* Add your preview container styles here */
            display: flex;
            overflow-x: auto;
            gap: 10px;
            padding: 10px;
        }

        .preview-item {
            /* Add your preview item styles here */
            position: relative;
            max-width: 200px;
            max-height: 150px;
            border: 1px solid #ccc;
            overflow: scroll;
        }

        .preview-item img,
        .preview-item video {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

        .btn-close {
            /* Add your close button styles here */
            position: absolute;
            top: 5px;
            right: 5px;
            background: none;
            border: none;
            color: #fff;
            font-size: 1.5rem;
            cursor: pointer;
            z-index: 1;
            padding: 5px;
        }

        .btn-close:hover {
            color: #f00;
        }
    </style>

